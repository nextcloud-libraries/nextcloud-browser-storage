{
  "name": "@nextcloud/browser-storage",
  "version": "0.4.0",
  "description": "",
  "keywords": [
    "nextcloud"
  ],
  "homepage": "https://github.com/nextcloud-libraries/nextcloud-browser-storage#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/nextcloud-libraries/nextcloud-browser-storage"
  },
  "license": "GPL-3.0-or-later",
  "author": "Christoph Wurst",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc --build --clean && tsc",
    "build:doc": "typedoc --excludeNotExported --mode file --out dist/doc lib/index.ts && touch dist/doc/.nojekyll",
    "dev": "tsc --watch",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "test": "vitest",
    "ts:check": "tsc --noEmit"
  },
  "browserslist": [
    "extends @nextcloud/browserslist-config"
  ],
  "devDependencies": {
    "@nextcloud/eslint-config": "^9.0.0-rc.2",
    "@types/node": "^24.0.3",
    "eslint": "^9.29.0",
    "happy-dom": "^18.0.1",
    "typedoc": "^0.28.1",
    "typescript": "5.8.3",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": "^24 || ^22 || ^20"
  },
  "devEngines": {
    "runtime": {
      "name": "node",
      "version": "^22",
      "onFail": "error"
    },
    "packageManager": {
      "name": "npm",
      "version": "^10",
      "onFail": "error"
    }
